{
    "compilerOptions": {
        "baseUrl": "../src",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "lib": [
            "es2016",
            "es2017",
            "es6",
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "paths": {
            "sudoo-pencil#declare/*": ["declare/*"],
            "sudoo-pencil#pencil/*": ["pencil/*"],
            "sudoo-pencil#sketch/*": ["sketch/*"],
            "sudoo-pencil#util/*": ["util/*"],
        },
        "sourceMap": true,
        "strict": true,
        "target": "es6"
    }
}